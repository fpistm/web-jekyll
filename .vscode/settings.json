// Place your settings in this file to overwrite default and user settings.
{
  "files.associations": {
    "*.html": "liquid"
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "editor.renderWhitespace": "all",
  "files.trimTrailingWhitespace": true,
  "markdownlint.config": {
    "MD041": false
  },
  "cSpell.ignoreWords": [
    "aarch64",
    "armv6",
    "armv7l",
    "balenalib",
    "raspberrypi3",
    "raspberrypi4"
  ]
}
