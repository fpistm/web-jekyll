// Place your settings in this file to overwrite default and user settings.
{
  "files.associations": {
    "*.html": "liquid"
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "editor.renderWhitespace": "all",
  "files.trimTrailingWhitespace": true,
  "markdownlint.config": {
    "MD041": false
  },
  "cSpell.ignoreWords": [
    "Arduino",
    "Dockerfiles",
    "PYTHONHOME",
    "PYTHONPATH",
    "RPATH",
    "Somsubhra",
    "aarch64",
    "altinstall",
    "armv5te",
    "armv6",
    "armv7",
    "armv7l",
    "armv8",
    "balenalib",
    "cmake",
    "eabi",
    "gpasswd",
    "marm",
    "mfloat",
    "mkdir",
    "mthumb",
    "nofp",
    "npmjs",
    "otool",
    "pathsep",
    "raspberrypi3",
    "raspberrypi4",
    "readelf",
    "runpath",
    "softfp"
  ]
}
